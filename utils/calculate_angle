import numpy as np

def calculate_angle_x(left_eye:tuple,right_eye:tuple):

    x = np.array(left_eye)
    y = np.array(right_eye)

    proj = x[1]-y[1]

    if proj > 0:
        pos = True
    elif proj < 0:
        pos = False
    else:
        return 0

    proj = np.abs(proj)

    ct = 1/np.tan(proj/(x[0]-y[0]))

    if pos:
        return 90-np.degrees(np.arctan(ct))
    else:
        return np.degrees(np.arctan(ct))-90